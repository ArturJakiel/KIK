{
  "extends": ["airbnb", "prettier"],
    "env": {
        "browser": true,
        "es6": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2019,
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
      "no-console": 0,
      "no-underscore-dangle": 0,
      "func-names": 0,
      "no-process-exit": 0,
      "consistent-return":0,
      "no-unused-vars": ["warn", { "args": "none", "ignoreRestSiblings": false }]
    },
    "settings": {
      "import/resolver": {
        "node": {
          "moduleDirectory": [
            "node_modules",
            "src"
          ]
        }
      }
    }
}